pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- character constants
hero_icon = 1
enemy_icon = 2

hero_r = 3
hero_l = 4

enemy_r = 5
enemy_l = 6
enemy_r_m = 7
enemy_l_m = 8

empty = 9

-- initial state
hero = {}
hero.lives = 3

killed_count = 0
is_gameover = false

function _init()
 hero.x = 60
 hero.y = 64
 hero.jump = -1
 hero.state = hero_r
 
 e1 = {}
 e1.x = -8
 e1.y = 64
 e1.state = enemy_r

 e2 = {}
 e2.x = 128
 e2.y = 64
 e2.state = enemy_l
 
 enemies = { e1, e2 }
end

function _update()
 if (is_gameover) return
 
 kill_enemies()
 move_hero()
 jump()
 move_enemies()
end

function _draw()
 if (is_gameover) return
 
 cls()
 map(0, 0)

 spr(hero_icon, 2, 2)
 print(hero.lives, 12, 2)
 spr(enemy_icon, 18, 2)
 print(killed_count, 28, 2)

 for i = 1, #enemies
 do
  e = enemies[i]
  spr(e.state, e.x, e.y)
 end
 spr(hero.state, hero.x, hero.y)
end

function kill_enemies()
 for i = 1, #enemies
 do
  e = enemies[i]
  if (e.state ~= empty)
  then
    if ((e.state == enemy_r or e.state == enemy_l) and hero.y + 8 > e.y and hero.x + 8 > e.x and hero.x < e.x + 8)
    then
     get_damage()
     return
    end
    if (hero.y + 8 <= e.y and hero.x + 8 >= e.x and hero.x <= e.x + 8)
    then
      if (e.state == enemy_r)
      then
       e.state = enemy_r_m
       killed_count += 1
      elseif (e.state == enemy_l)
      then
       e.state = enemy_l_m
       killed_count += 1
      end
    elseif (e.state == enemy_r_m)
    then
     e.x = -8
     e.state = enemy_r
    elseif (e.state == enemy_l_m)
    then
     e.x = 128
     e.state = enemy_l
    end
  end
 end
end

function move_hero()
 if (btn(0) and hero.x > 0)
 then
  hero.x -= 1
  hero.state = hero_l
 end
 
 if (btn(1) and hero.x < 120)
 then
  hero.x += 1
  hero.state = hero_r
 end
 
 if (btn(5))
 then
  if (hero.jump == -1)
  then
   hero.jump = 20
   jump()
  end 
 end
end

function jump()
 if (hero.jump ~= -1)
 then
  if (hero.jump % 2 == 0)
   then
	  if (hero.jump >= 12)
	   then hero.y -= 3
	  elseif (hero.jump <= 8)
	   then hero.y += 3
	  end
	 end
  hero.jump -= 1
 end
end

function move_enemies()
 for i = 1, #enemies
 do
  e = enemies[i]
  if (e.state == enemy_r)
  then
   e.x += 1
  elseif (e.state == enemy_l)
  then
   e.x -= 1
  elseif (e.type == 1)
  then
   if (e.x > 128)
   then
    e.x = -8
    e.state = enemy_r
   end
   e.x += 1
  elseif (e.type == 2)
  then
   if (e.x < -8)
   then
    e.x = 128
    e.state = enemy_l
   end
   e.x -= 1
  end
 end
end

function get_damage()
 init_state = hero.state
 for i = 1, 20
 do
  if (hero.state ~= empty)
  then
   hero.state = empty
  else
   hero.state = init_state
  end
  _draw()
  wait(1)
 end
 if (hero.lives > 0)
 then
  hero.lives -= 1
  _init()
  _draw()
 else
  cls()
  print('game over', 46, 55)
  print('press cmd+r to start', 24, 65)
  is_gameover = true
 end
end

function wait(time)
 for i = 1, time
 do
  flip()
 end
end
